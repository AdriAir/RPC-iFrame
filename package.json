{
    "name": "iframeconnector",
    "version": "0.0.1",
    "description": "Modern JS/TS library for RPC-style communication between parent and iframe",
    "license": "MIT",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts && cpx \"dist/**/*\" examples/dist",
        "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
        "typecheck": "tsc --noEmit",
        "clean": "rimraf dist examples/dist",
        "serve": "serve examples/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AdriAir/iFrameConnector.git"
    },
    "bugs": {
        "url": "https://github.com/AdriAir/iFrameConnector/issues"
    },
    "homepage": "https://github.com/AdriAir/iFrameConnector#readme",
    "devDependencies": {
        "cpx": "^1.5.0",
        "rimraf": "^6.1.2",
        "serve": "^14.2.5",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    }
}
